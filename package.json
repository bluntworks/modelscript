  "version": "2.3.4",
  "commonjs": "build/modelscript.cjs.js",
  "esm": "build/modelscript.esm.js",
  "main": "index.mjs",
  "browser": "build/modelscript.umd.js",
  "modules.root": "src",
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "csvtojson": "^1.1.9",

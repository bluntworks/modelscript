    "babel-preset-es2017": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.0.2",
    "coveralls": "^3.0.2",
    "esdoc": "^1.1.0",
